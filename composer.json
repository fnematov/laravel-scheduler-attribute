{
    "name": "fnematov/laravel-scheduler-attribute",
    "description": "Laravel schedule registerer by attributes",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "scheduler",
        "attribute"
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "symfony/finder": "^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Fnematov\\LaravelSchedulerAttribute\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fnematov\\LaravelSchedulerAttribute\\LaravelSchedulerAttributeProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Farhod Nematov",
            "email": "mr.nematov91@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev"
}
